<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <application
            android:allowBackup="true"
            android:dataExtractionRules="@xml/data_extraction_rules"
            android:fullBackupContent="@xml/backup_rules"
            android:icon="@drawable/appimg"
            android:label="@string/app_name"
            android:roundIcon="@drawable/appimg"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            tools:targetApi="31">
        <activity
                android:name=".StoreOrderActivity"
                android:label="This is the StoreOrder activity"
                android:parentActivityName=".MainActivity"
                android:exported="false" />
        <activity
                android:name=".OrderActivity"
                android:label="This is the Order activity"
                android:parentActivityName=".MainActivity"
                android:exported="false" />
g        <activity
                android:name=".DonutActivity"
                android:label="This is the Donut activity"
                android:parentActivityName=".MainActivity"
                android:exported="false" />
        <activity
                android:name=".CoffeeActivity"
                android:label="This is the Coffee activity"
                android:parentActivityName=".MainActivity"
                android:exported="false" />
        <activity
                android:name=".MainActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

            <meta-data
                    android:name="android.app.lib_name"
                    android:value=""/>
        </activity>
    </application>

</manifest>